{
  "compact": false,
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": { "node": "6" },
        "loose": true,
        "exclude": [
          "transform-regenerator"
        ]
      }
    ]
  ],
  "plugins": [
    "babel-plugin-add-module-exports",
    "@babel/plugin-transform-async-to-generator"
  ]
}
